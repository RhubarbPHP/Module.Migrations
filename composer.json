{
  "name": "rhubarbphp/module-migrations",
  "description": "Provides scripted updates for managed transitions in application versions",
  "keywords": [
    "version",
    "script",
    "rhubarb",
    "php",
    "framework",
    "migration"
  ],
  "homepage": "http://www.rhubarbphp.com/",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Rhubarb\\Modules\\Migrations\\": "src/",
      "Rhubarb\\Modules\\Migrations\\Tests\\": "tests/unit"
    }
  },
  "require": {
    "rhubarbphp/rhubarb": "^1.5.9",
    "rhubarbphp/module-stem": "*",
    "rhubarbphp/custard": "*",
    "rhubarbphp/scaffold-application-settings": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "codeception/codeception": "*",
    "codeception/verify": "*"
  },
  "config": {
    "bin-dir": "bin/"
  }
}
